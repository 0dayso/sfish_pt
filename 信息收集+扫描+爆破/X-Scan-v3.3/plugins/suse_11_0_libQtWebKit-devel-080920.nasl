include("compat.inc");

if ( ! defined_func("bn_random") ) exit(0);

if(description)
{
 script_id(40021);
 script_version("$Revision: 1.3 $");
 script_name(english: "SuSE 11.0 Security Update:  libQtWebKit-devel (2008-09-20)");
 script_set_attribute(attribute: "synopsis", value: 
"The remote SuSE system is missing a security patch for libQtWebKit-devel");
 script_set_attribute(attribute: "description", value: "A flaw in the CSS loader of the WebKit engine could crash
programs and potentially allows execution of arbitrary code
(CVE-2008-3632).

This update also fixes unrelated problems with printing.
");
 script_set_attribute(attribute: "cvss_vector", value: "CVSS2#AV:N/AC:M/Au:N/C:C/I:C/A:C");
script_set_attribute(attribute: "solution", value: "Run yast to install the security patch for libQtWebKit-devel");
script_set_attribute(attribute: "see_also", value: "https://bugzilla.novell.com/show_bug.cgi?id=426919");
script_set_attribute(attribute: "see_also", value: "https://bugzilla.novell.com/show_bug.cgi?id=384674");
script_end_attributes();

 script_cve_id("CVE-2008-3632");
script_summary(english: "Check for the libQtWebKit-devel package");
 
 script_category(ACT_GATHER_INFO);
 
 script_copyright(english:"This script is Copyright (C) 2009 Tenable Network Security, Inc.");
 script_family(english: "SuSE Local Security Checks");
 script_dependencies("ssh_get_info.nasl");
 script_require_keys("Host/SuSE/rpm-list");
 exit(0);
}

