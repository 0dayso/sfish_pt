 script_set_attribute(attribute: "description", value: "Fixed a format string vulnerability in smbclient
(CVE-2009-1886) and a ACL bypass vulnerability in samba
(CVE-2009-1888).
");
 script_set_attribute(attribute: "cvss_vector", value: "CVSS2#AV:N/AC:M/Au:N/C:C/I:C/A:C");
script_set_attribute(attribute: "solution", value: "Run yast to install the security patch for cifs-mount");
script_set_attribute(attribute: "see_also", value: "https://bugzilla.novell.com/show_bug.cgi?id=515479");
script_set_attribute(attribute: "see_also", value: "https://bugzilla.novell.com/show_bug.cgi?id=513360");
script_end_attributes();

 script_cve_id("CVE-2009-1886", "CVE-2009-1888");
script_summary(english: "Check for the cifs-mount package");
 
 script_category(ACT_GATHER_INFO);
 
 script_copyright(english:"This script is Copyright (C) 2009 Tenable Network Security, Inc.");
 script_family(english: "SuSE Local Security Checks");
 script_dependencies("ssh_get_info.nasl");
 script_require_keys("Host/SuSE/rpm-list");
 exit(0);
}

